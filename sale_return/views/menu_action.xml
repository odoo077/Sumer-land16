<?xml version="1.0" encoding="utf-8"?>
<odoo>

      <record id="seq_sale_order_return" model="ir.sequence">
            <field name="name">Sales Order return</field>
            <field name="code">sale.order.return</field>
            <field name="prefix">SR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    <record id="sale_order_return_action" model="ir.actions.act_window">
        <field name="name">Sale Retun</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">sale.order.return</field>
        <field name="view_mode">tree,form</field>
    </record>
    <!-- This Menu Item must have a parent and an action -->
    <menuitem id="menu_sale_order_return" name="Sale Retun" parent="sale.sale_menu_root" action="sale_order_return_action" sequence="5"/>
</odoo>